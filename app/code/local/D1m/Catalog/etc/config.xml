<?xml version="1.0"?>
<config>
    <modules>
        <D1m_Catalog>
            <version>0.1.1</version>
        </D1m_Catalog>
    </modules>

    <global>
        <resources>
            <d1m_catalog_setup>
                <setup>
                    <module>D1m_Catalog</module>
                    <class>Mage_Catalog_Model_Resource_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </d1m_catalog_setup>
            <d1m_catalog_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </d1m_catalog_write>
            <d1m_catalog_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </d1m_catalog_read>
        </resources>

    </global>

</config>
